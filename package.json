{
    "name"            : "brewmymac",
    "description"     : "The coolest way to install apps and customize your Mac!",
    "version"         : "2.0.0",
    "author"          : {
        "name" : "Alan Tai"
    },
    "repository"      : {
        "type" : "git",
        "url"  : "git+https://github.com/ayltai/BrewMyMac.git"
    },
    "bugs"            : {
        "url" : "https://github.com/ayltai/BrewMyMac/issues"
    },
    "license"         : "MIT",
    "keywords"        : [
        "brew",
        "homebrew",
        "install",
        "installation",
        "mac",
        "macos",
        "package manager"
    ],
    "homepage"        : "https://brewmymac.sh",
    "scripts"         : {
        "clean"   : "pnpm dlx rimraf --glob **/node_modules pnpm-lock.yaml package-lock.json",
        "reset"   : "pnpm run clean && pnpm i && pnpm audit -P",
        "dev"     : "cross-env NEXT_TELEMETRY_DISABLED=1 && next",
        "build"   : "cross-env NEXT_TELEMETRY_DISABLED=1 && next build",
        "lint"    : "cross-env NEXT_TELEMETRY_DISABLED=1 && next lint",
        "pretest" : "pnpm run lint",
        "test"    : "jest --maxWorkers=100%",
        "test:ci" : "jest --runInBand",
        "start"   : "cross-env NEXT_TELEMETRY_DISABLED=1 && next start",
        "update"  : "pnpm dlx npm-check-updates"
    },
    "dependencies"    : {
        "@emotion/cache"      : "^11.11.0",
        "@emotion/react"      : "^11.11.0",
        "@emotion/server"     : "^11.11.0",
        "@emotion/styled"     : "^11.11.0",
        "@mui/icons-material" : "^5.11.16",
        "@mui/material"       : "^5.13.0",
        "@reduxjs/toolkit"    : "^1.9.5",
        "@sentry/nextjs"      : "^7.51.2",
        "i18next"             : "^22.4.15",
        "minisearch"          : "^6.0.1",
        "mixpanel-browser"    : "^2.47.0",
        "nanoid"              : "^4.0.2",
        "next"                : "^13.4.1",
        "next-i18next"        : "^13.2.2",
        "react"               : "^18.2.0",
        "react-dom"           : "^18.2.0",
        "react-i18next"       : "^12.2.2",
        "react-redux"         : "^8.0.5",
        "redux-persist"       : "^6.0.0",
        "usehooks-ts"         : "^2.9.1"
    },
    "devDependencies" : {
        "@testing-library/jest-dom"        : "^5.16.5",
        "@testing-library/react"           : "^14.0.0",
        "@types/jest"                      : "^29.5.1",
        "@types/mixpanel-browser"          : "^2.38.1",
        "@types/node"                      : "^20.1.2",
        "@types/react"                     : "^18.2.6",
        "@types/react-dom"                 : "^18.2.4",
        "@types/react-test-renderer"       : "^18.0.0",
        "@typescript-eslint/eslint-plugin" : "^5.59.5",
        "cross-env"                        : "^7.0.3",
        "css-mediaquery"                   : "^0.1.2",
        "eslint"                           : "^8.40.0",
        "eslint-config-next"               : "^13.4.1",
        "eslint-plugin-jest"               : "^27.2.1",
        "eslint-plugin-jest-dom"           : "^4.0.3",
        "eslint-plugin-testing-library"    : "^5.11.0",
        "jest"                             : "^29.5.0",
        "jest-environment-jsdom"           : "^29.5.0",
        "jest-fetch-mock"                  : "^3.0.3",
        "react-test-renderer"              : "^18.2.0",
        "typescript"                       : "^5.0.4"
    },
    "engines"         : {
        "node" : ">=18",
        "pnpm" : ">=8"
    },
    "browserslist"    : {
        "production"  : [
            ">0.5%",
            "not dead",
            "not Explorer <= 11",
            "not OperaMini all",
            "Firefox ESR"
        ],
        "development" : [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
